{
  "name": "dcm-meeting-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "cross-env PORT=80 nodemon nodemon",
    "dev": "cross-env NODE_ENV=dev PORT=5000 npm run start",
    "test": "cross-env NODE_ENV=dev jest",
    "test-watch": "cross-env NODE_ENV=dev jest --watchAll"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register src/index.ts",
    "ext": "ts, js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "fs-extra": "^11.2.0",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
